{
  "name": "styled-react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal & Modern boilerplate for building apps with React & styled-components",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "build": "webpack --mode production",
    "test": "xo && stylelint './src/**/*.js'"
  },
  "main": "src/index.js",
  "repository": "https://github.com/xxczaki/styled-react-boilerplate",
  "author": "Antoni Kepinski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.0",
    "img-loader": "^3.0.1",
    "script-ext-html-webpack-plugin": "^2.0.3",
    "stylelint": "^9.7.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "xo": "^0.23.0"
  },
  "dependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-icons": "^3.2.2",
    "react-router-dom": "^4.3.1",
    "styled-components": "^4.0.3"
  },
  "xo": {
    "extends": "xo-react",
    "envs": "browser",
    "ignores": "src/index.js",
    "esnext": true,
    "rules": {
      "react/no-unescaped-entities": null
    }
  }
}
